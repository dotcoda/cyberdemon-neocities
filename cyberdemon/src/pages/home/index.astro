---
import HomeLayout from "../../layouts/HomeLayout.astro";
import '../../styles/home.css';
---
<script src="/scripts/click.js" is:inline></script>

<script src="/scripts/wobble.js" is:inline></script>
<link href="https://fonts.googleapis.com/css2?family=Material+Icons" rel="stylesheet">

<HomeLayout pageName="CYBERDEMON">
<div class="flex">
    <div class="flexChild child-equal">
        <!-- top left welcome -->
        <div id="welcome" class="pixel radius-10 border"> 
            <div id="header" class="radius-10 border bg-white">
                <img class="radius-5 border" src="../images/graphics/wiiFrisbee.png"
                    alt="a nintendo wii dog holding a frisbee on a beach">
            </div>
            <p class="radius-10 border bg-white">
                <img src="images/pixels/f1001751-49727b05.gif" aria-hidden="true" alt=""> hello
                and welcome to <span id="wobble">cyberdemon.neocities.org</span> v5! <img
                    src="images/pixels/f429989.gif" alt="" aria-hidden="true">
                my name is <a href="/about" onmouseover="mouseoversound.playclip()"
                    onclick="clicksound.playclip()">coda </a> <img src="images/pixels/f70603.gif"
                    aria-hidden="true" alt="">
                and im the webmaster
                <img src="https://64.media.tumblr.com/e1e6e5ba9a0ae3feabe0bc105d521487/ee247f500ad639c0-0b/s75x75_c1/c0d47cc1802849076ea42c0215e07abc3b6b6d37.gifv"
                    aria-hidden="true" alt="">
                enjoy your stay in my digital dream world <img src="images/pixels/f157698.gif"
                    aria-hidden="true" alt="">
            </p>
            <img id="cool" src="../images/pixels/joeCool.gif" aria-hidden="true" alt="">


        </div>

        <!-- top left status cafe -->
        <div id="status" class="radius-10 border">
            <div id="tip">
                <div style="z-index: 3; padding: 10px; margin-bottom: -5px;" role="tooltip"
                    class="is-top is-left">
                    <p>the webmaster is feeling: <a href="https://www.imood.com/users/coda"><img
                                style="margin-bottom: -3px; height:1rem; width:auto"
                                src="https://moods.imood.com/display/uname-coda/trans-1/imood.gif"
                                alt="The current mood of coda at www.imood.com"></a>
                    </p>
                </div>
            </div>

            <div id="statusContainer" class="radius-10 border bg-white">
                <div id="statuscafe">
                    <img src="../images/graphics/arf.png" class="drop-shadow"
                        alt="a png of a nintendog with a the status as a speech bubble">
                    <div style="text-align: center;" id="statuscafe-username"></div>
                    <div
                        id="statuscafe-content" class="radius-5"></div>
                </div>
                <script src="https://status.cafe/current-status.js?name=cyberdemon"
                    defer></script>

            </div>
        </div>
    </div>

    <div class="flexChild child-half" style="position: relative;">
       
        <!-- Audio Element -->
        <div class="mobileHidden radius-5 border">
            <img style="width: 100%; height: auto;" data-toggle="tooltip"
                title="click the button below to play a song!"
                alt="a tv with the face of an 8bit woman talking on it"
                src="https://64.media.tumblr.com/5de59d0bd70e4e1963745d86b3ea7051/e59782c17ffb62ce-e2/s640x960/5b12857d3978538e695967df4a768e1d036233e1.gifv">

                <div style="display: flex; align-items: center; gap: 10px;">
                    
                    <audio id="myAudio" src="https://cyberdemon.nekoweb.org/audio/retro tv.mp3" type="audio"></audio>
                
                    <span style="cursor:pointer; color:#3399ff; font-size:12px;" id="music" onclick="toggleAudio()">
                        <span class="material-icons" id="icon">play_arrow</span>
                    </span>
                
                    <div id="nowPlaying" class="scroll-container">
                        <span class="scrolling-text">Now Playing: Retro TV - Animal Crossing</span>
                    </div>
                
                    <span style="cursor:pointer; color:#3399ff; font-size:12px;" onclick="nextTrack()">
                        <span class="material-icons">skip_next</span>
                    </span>
                </div>
                
               <script src="/scripts/audio.js" is:inline></script>
        </div>

        <!-- top right about -->
        <div id="about" class="border radius-10">
            <img id="mii" src="/images/graphics/mii.gif" class="drop-shadow" alt="a red haired mii face">

            <p class="bg-white border radius-10 child-equal">
                coda <br>
                twenty-one<br>
                they/any <img class="border"
                    alt="trans flag"
                    src="https://images-wixmp-ed30a86b8c4ca887773594c2.wixmp.com/f/2dbd0ffa-0723-42f6-8cb3-5375aa7451c8/dfxr2pc-1a985cd1-952d-469e-9e44-f01fb53292d7.png?token=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9..m5SpKr9fgWRs0Y7hEfsRw9V7AkKv66jwukY04vbPtK4">
                <br>
            </p>
        </div>

        <!-- top right link -->
        <h2>link me!</h2>
        <div id="link" class="border bg-white radius-5">
            <center> <img class="w-auto" src="https://cyberdemon.neocities.org/button.png"
                    alt="the button of cyberdemon"></center>
            <textarea id="myInput"
                style="height:30px; width:93%; margin-top:5px; margin-left:5px;"
                id="myInput">&lta href="https://cyberdemon.neocities.org/"&gt&ltimg src="https://cyberdemon.neocities.org/button.png" alt="the button of cyberdemon"&gt&lt/a&gt</textarea>
            <button style="float:left; margin-left:5px; margin-right:5px"
                onclick="copy()">Copy</button>
            <script>
                function copy() {
                    // Get the text field
                    var copyText = document.getElementById("myInput");

                    // Select the text field
                    copyText.select();
                    copyText.setSelectionRange(0, 99999); // For mobile devices

                    // Copy the text inside the text field
                    navigator.clipboard.writeText(copyText.value);
                }
            </script>
        </div>

        <img class="fish mobileHidden" 
        aria-hidden="true" alt=""
        src="https://images-wixmp-ed30a86b8c4ca887773594c2.wixmp.com/f/9cbba6e5-cffe-4537-a24a-305b77e34fb7/djkjujv-baecf96c-ae18-4691-b344-16a24403cd72.gif?token=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9..xQkN3Y0Pm46XSS8MTHqzvfFS5P0Ni1tq0m9v2ICq7aM">
        

    </div>


<!-- bottom layer -->
   

</div>
