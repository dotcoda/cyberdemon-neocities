            <h2>artists ive met</h2>
    
            <p class="border shows">
                <mark>9.14.19</mark>the garden<br>
                <mark>9.15.19</mark>frank iero<br>
                <mark>9.16.19</mark>dallon weeks<br>
            </p>
           
    
            <div class="window active" style="max-width: 300px">
                <div class="title-bar">
                  <div class="title-bar-text">Last.fm</div>
                  <div class="title-bar-controls">
                    <button aria-label="Minimize"></button>
                    <button aria-label="Maximize"></button>
                    <button aria-label="Close"></button>
                  </div>
                </div>
                <div class="window-body has-space">
                    <a href="https://www.last.fm/user/candidangel" target="_blank"><img src="https://lastfm-recently-played.vercel.app/api?user=candidangel" height="auto" width="100%"/></a>
                </div>
              </div>
    
    
           <img class="listening" src="../../images/graphics/music.png">
            
            </div><!--end second tier-->


</Layout>